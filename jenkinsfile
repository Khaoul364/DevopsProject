pipeline {

    agent {label "vagrantagent"}
    stages {

        stage ('GIT') {
            steps {
               echo "Getting Project from Git"; 
                git branch: "LindaBranch", 
                    url: "https://github.com/Khaoul364/DevopsProject";
              }
         }
       
        stage("Clean") {
            steps {
                sh "mvn -version"
                sh "mvn clean package -DskipTests"
            }
        }
         stage("Build") {
            steps {
                sh "mvn compile"
            }
        }
        //   stage("Test") {
        //     steps {
        //         sh "mvn test"
        //     }
        // }

        stage("Build Docker image") {
            steps {
                sh "sudo docker build -t kaddem ."
            }
        }
         stage("Sonar") {
         steps {
              sh "mvn sonar:sonar -Dsonar.login=admin -Dsonar.password=admin123"
          }
         }

     //  stage("Deploy Artifact to private registry") {
       //     steps {
            //    sh 'docker run  -p 9090:9090--network mynetwork --name -d spring-container spring1-image'
         //   }
        //}
    // stage("Nexus") {
    //   steps{
    //         sh "mvn deploy -DskipTests"
    //   }
    // }
   
    }

}
